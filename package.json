{
  "name": "asp-pw",
  "version": "1.0.0",
  "description": "A password hash and validation library in Node compatible with the ASP.NET identity framework.",
  "main": "lib/asp-pw.js",
  "scripts": {
    "test": "jest",
    "build": "npm run clear-lib && npm run build-lib",
    "build-lib": "babel src --out-dir lib --ignore src/__tests__/**",
    "clear-lib": "rm -rf lib"
  },
  "keywords": [
    "asp",
    "password",
    "hash",
    "identity"
  ],
  "engines": {
    "node": ">=6.9.4"
  },
  "author": "Kelly J Andrews (kelly@kellyjandrews.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^2.0.0-rc.1",
    "jest": "^18.1.0"
  }
}
